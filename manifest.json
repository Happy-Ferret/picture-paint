{
    "manifest_version": 2,
    "name": "Picture Paint",
    "version": "0.1.0",
    "author": "Danny Guo",
    "homepage_url": "https://github.com/dguo/picture-paint",
    "applications": {
        "gecko": {
            "id": "@picture-paint",
            "strict_min_version": "60.0a1"
